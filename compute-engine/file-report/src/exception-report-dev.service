[Unit]
Description=Exception Report Dev
Requires=network.target
After=multi-user.target

[Service]
Restart=on-failure
Type=simple
EnvironmentFile=/opt/exception-report/github/compute-engine/file-report/src/env-dev.txt
ExecStart=/opt/exception-report/github/compute-engine/file-report/src/exception-report
StandardOutput=/var/log/exception-report/dev.log
StandardError=/var/log/exception-report/dev.log

[Install]
WantedBy=multi-user.target